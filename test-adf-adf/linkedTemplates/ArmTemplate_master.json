					"AzureBlobStorage988_properties_privateLinkResourceId": {
						"value": "[parameters('AzureBlobStorage988_properties_privateLinkResourceId')]"
					},
					"AzureBlobStorage988_properties_groupId": {
						"value": "[parameters('AzureBlobStorage988_properties_groupId')]"
					},
					"AzureBlobStorage988_properties_fqdns": {
						"value": "[parameters('AzureBlobStorage988_properties_fqdns')]"
					},
					"AzureSqlDatabase535_properties_privateLinkResourceId": {
						"value": "[parameters('AzureSqlDatabase535_properties_privateLinkResourceId')]"
					},
					"AzureSqlDatabase535_properties_groupId": {
						"value": "[parameters('AzureSqlDatabase535_properties_groupId')]"
					},
					"AzureSqlDatabase535_properties_fqdns": {
						"value": "[parameters('AzureSqlDatabase535_properties_fqdns')]"
